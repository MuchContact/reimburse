<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="tw.pc.mapper.ExpenseMapper">
    <insert id="createExpense">insert into expense(name, price) values(#{expense.name}, #{expense.price})</insert>
    <select id="getExpenseById" resultType="tw.pc.domain.Expense"></select>
</mapper>